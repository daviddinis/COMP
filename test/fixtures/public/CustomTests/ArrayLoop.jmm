
class ArrayLoop {

    int field;

    public static void main(String[] args) {

        int[] x;
        int a;
        ArrayLoop object;
        int[] arr;

        arr = new int[1];
        x = new int[10];
        a = 0;
        object = new ArrayLoop();

        
        //Prints 0->9
        while(a < x.length){
            x[a] = a;
            io.println(x[a]);
            a = a + 1;
        }
        
        //Prints (9->0) *2)
        a = x.length - 1;
        while( 0 - 1 < a ){
            x[a] = object.func(a);
            io.println(x[a]);
            a = a - 1;
        }
        
        
        io.println(x[9] * x[9]);
        
        arr[0] = 99;
        //Array addition ( 99 + 2 )
        x[0] = arr[0] + x[1];
        io.println(x[0]);
        
        //Prints 6
        io.println(x[2+3-2]);
	}

    public int func(int k){
       return k * 2;
    }


    public ArrayLoop getInstance(){
        return new ArrayLoop();
    }

    public int setField(int fieldValue){
        field = fieldValue;
        return 0;
    }

    public int getField(){
        return field;
    }
}